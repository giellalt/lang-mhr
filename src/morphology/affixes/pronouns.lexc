!! !!!Eastern Mari pronoun inflection

!! !!Lexica directed from root.lexc

LEXICON pronouns_not_from_xml !!= * @CODE@
мый+Pron+Pers: MYJ ;	
тый+Pron+Pers: TYJ ;	
тудо+Pron: TUDO ;	
тиде+Pron: TIDE ;	
шке+Pron+Refl: SHKE ;

LEXICON MYJ !!= * @CODE@
+Sg+Nom:мый # ;
+Sg+Gen:мыйын # ;
+Sg+Der/Poss:мыйын N_ ;
+Sg+Acc:мыйым # ;
+Sg+Dat:мыланем #;
+Sg+Com:мыйге # ;
+Sg+Cmpr:мыйла # ;
+Pl+Nom:ме # ;
+Pl+Gen:мемнан # ;
+Pl+Der/Poss:мемнан N_ ;
+Pl+Acc:мемнам # ;
+Pl+Dat:мыланна #;
+Pl+Dat+Use/NG:мемналан #;
+Pl+Com:меге # ;
+Pl+Cmpr:мела # ;

LEXICON TYJ !!= * @CODE@
+Sg+Nom:тый # ;
+Sg+Gen:тыйын # ;
+Sg+Der/Poss:тыйын N_ ;
+Sg+Acc:тыйым # ;
+Sg+Dat:тыланет #;
+Sg+Com:тыйге # ;
+Sg+Cmpr:тыйла # ;
+Pl+Nom:те # ;
+Pl+Gen:темнан # ;
+Pl+Der/Poss:темнан N_ ;
