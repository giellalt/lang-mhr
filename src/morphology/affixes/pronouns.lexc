!! !!!Eastern Mari pronoun inflection

!! !!Lexica directed from root.lexc

LEXICON pronouns_not_from_xml !!= * @CODE@
мый+Pron+Pers: MYJ ;	
тый+Pron+Pers: TYJ ;	
тудо+Pron: TUDO ;	
тиде+Pron: TIDE ;	
шке+Pron+Refl: SHKE ;
кажне+Pron+Indef: KAZHNE ;
кеч-мо:кеч-мо PRON-INDEF ;
кеч-кӧ:кеч-кӧ PRON-INDEF ;
нимо:нимо PRON-INDEF ;
нигӧ:нигӧ PRON-INDEF ;
ала-мо:ала-мо PRON-INDEF ;
ала-кӧ:ала-кӧ PRON-INDEF ;
кеч-мо:кеч-мо PRON-INDEF ;
кеч-кӧ:кеч-кӧ PRON-INDEF ;
керек-мо:керек-мо PRON-INDEF ;
керек-кӧ:керек-кӧ PRON-INDEF ;
иктаж-мо:иктаж-мо PRON-INDEF ;
иктаж-кӧ:иктаж-кӧ PRON-INDEF ;

мо+Pron+Interr+Sg+PxSg2:моч N_KIN_SPECIAL ;
нимо+Pron+Neg+Sg+PxSg2:нимоч N_KIN_SPECIAL ;
кеч-мо+Pron+Indef+PxSg2:кеч-моч N_KIN_SPECIAL ;
керек-мо+Pron+Indef+PxSg2:керек-моч N_KIN_SPECIAL ;
ала-мо+Pron+Indef+PxSg2:ала-моч N_KIN_SPECIAL ;
иктаж-мо+Pron+Indef+PxSg2:иктаж-моч N_KIN_SPECIAL ;

LEXICON MYJ !!= * @CODE@
+Sg1+Nom:мый K_at ;
+Sg1+Nom+Use/NG:мые # ;
+Sg1+Gen:мыйын K_at ;
+Sg1+Der/Poss:мыйын N-ava_01_obl ;
+Sg1+Acc:мыйым K_at ;
+Sg1+Dat+Use/NG:мыланем K_at;
+Sg1+Dat:мылам K_at;
+Sg1+Com:мыйге # ;
+Sg1+Cmpr:мыйла # ;
+Pl1+Nom:ме K_at ;
+Pl1+Gen:мемнан K_at ;
+Pl1+Der/Poss:мемнан N-ava_01_obl ;
+Pl1+Acc:мемнам K_at ;
+Pl1+Dat:мыланна K_at ;
+Pl1+Dat+Use/NG:мемналан #;
+Pl1+Com:меге # ;
+Pl1+Cmpr:мела # ;

LEXICON TYJ !!= * @CODE@
+Sg2+Nom:тый K_at ;
+Sg2+Nom+Use/NG:тые # ;
+Sg2+Gen:тыйын K_at ;
+Sg2+Der/Poss:тыйын N-ava_01_obl ;
+Sg2+Acc:тыйым K_at ;
+Sg2+Dat+Use/NG:тыланет K_at;
+Sg2+Dat:тылат K_at;
+Sg2+Com:тыйге # ;
+Sg2+Cmpr:тыйла # ;
+Pl2+Nom:те K_at ;
+Pl2+Gen:тендан K_at ;
+Pl2+Der/Poss:тендан N-ava_01_obl ;
+Pl2+Acc:тендам K_at ;
+Pl2+Dat:тыланда K_at;
+Pl2+Dat+Use/NG:тендалан #;
+Pl2+Com:теге # ;
+Pl2+Cmpr:тела # ;

LEXICON TUDO !!= * @CODE@
+Pers+Sg3+Nom:туд%{оы%} K_at ;
+Pers+Sg3+Gen:тудын K_at ;
+Sg+Der/Poss:тудын N-ava_01_obl ;
