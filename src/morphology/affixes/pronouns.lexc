!! !!Pronoun lexica from xml

LEXICON Pimp  !!= * @CODE@
+Pron: K ;

LEXICON Pmuu !!= * @CODE@
+Interj: K ; ! Several Pmuu do not belong here

LEXICON PRON-IR_ !!= * @CODE@
+Pron+Interr: K ;
+Pron+Rel: K ;

LEXICON PRON_ !!= * @CODE@
+Pron: K ;

LEXICON PronRes !!= * @CODE@
+Pron: K ;

LEXICON PronI !!= * @CODE@
+Pron: # ;

LEXICON PronIR !!= * @CODE@
+Pron+Interr: # ;

LEXICON PRON-INTERR/REL_
+Pron+Interr: N-ava_01 ;
+Pron+Rel: N-ava_01 ;


LEXICON PronInd !!= * @CODE@
+Pron+Ind: # ;

LEXICON PronDem !!= * @CODE@
+Pron+Dem: # ;

LEXICON PronRef !!= * @CODE@
+Pron+Refl: # ;

