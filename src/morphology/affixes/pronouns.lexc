!! !!!Eastern Mari pronoun inflection

!! !!Lexica directed from root.lexc

LEXICON pronouns_not_from_xml !!= * @CODE@
мый+Pron+Pers: MYJ ;	
тый+Pron+Pers: TYJ ;	
тудо+Pron: TUDO ;	
тиде+Pron: TIDE ;	
шке+Pron+Refl: SHKE ;

LEXICON MYJ !!= * @CODE@
+Sg+Nom:мый K_at ;
+Sg+Gen:мыйын K_at ;
+Sg+Der/Poss:мыйын N-ava_01_obl ;
+Sg+Acc:мыйым K_at ;
+Sg+Dat:мыланем #;
+Sg+Com:мыйге # ;
+Sg+Cmpr:мыйла # ;
+Pl+Nom:ме K_at ;
+Pl+Gen:мемнан K_at ;
+Pl+Der/Poss:мемнан N-ava_01_obl ;
+Pl+Acc:мемнам K_at ;
+Pl+Dat:мыланна K_at ;
+Pl+Dat+Use/NG:мемналан #;
+Pl+Com:меге # ;
+Pl+Cmpr:мела # ;

LEXICON TYJ !!= * @CODE@
+Sg+Nom:тый K_at ;
+Sg+Gen:тыйын K_at ;
+Sg+Der/Poss:тыйын N-ava_01_obl ;
+Sg+Acc:тыйым K_at ;
+Sg+Dat:тыланет #;
+Sg+Com:тыйге # ;
+Sg+Cmpr:тыйла # ;
+Pl+Nom:те K_at ;
+Pl+Gen:темнан K_at ;
+Pl+Der/Poss:темнан N-ava_01_obl ;
+Pl+Acc:темнам K_at ;
+Pl+Dat:тыланна #;
+Pl+Dat+Use/NG:тендалан #;
+Pl+Com:теге # ;
+Pl+Cmpr:тела # ;

LEXICON TUDO !!= * @CODE@
+Pers+Sg+Nom:тудо K_at ;
+Pers+Sg+Gen:тудын K_at ;
+Sg+Der/Poss:тудын N-ava_01_obl ;
+Pers+Sg+Acc:тудым K_at ;
+Pers+Sg+Dat:тудлан K_at;
+Pers+Sg+Com:тудыге # ;
+Pers+Sg+Cmpr:тудла # ;
+Pers+Pl+Nom:нуно K_at ;
+Pers+Pl+Gen:нунын K_at ;
+Pl+Der/Poss:нунын N-ava_01_obl ;
